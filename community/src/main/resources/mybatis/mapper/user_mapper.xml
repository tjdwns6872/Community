<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="com.simple.community.mapper.UserMapper" >
	<select id="getOne" resultType="map">
		SELECT USER_ID FROM TP_USER
	</select>
	
	<insert id="userJoin">
		ISERT INTO TP_USER
		(
			USER_NO
			,USER_ID
			,USER_PW
			,USER_NAME
			,USER_EMAIL
			,USER_PHONE
			,USER_GENDER
			,USER_RANK
			,USER_START_DATE
			,USER_CHECK
		)VALUES
		(
			USER_SEQ.NEXTVAL()
			,#{user_id}
			,#{user_pw}
			,#{user_name}
			,#{user_email}
			,#{user_phone}
			,#{user_gender}
			,0
			,SYSDATE
			,0
		)
	</insert>
</mapper>